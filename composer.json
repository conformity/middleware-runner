{
    "name": "ecfectus/middleware-runner",
    "description": "A simple PSR container aware invokable middleware runner based on Relay Runner",
    "homepage": "https://github.com/ecfectus/middleware-runner",
    "license": "MIT",
    "authors": [
        {
            "name": "leemason",
            "email": "contact@nohalfpixels.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "container-interop/container-interop": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "fabpot/php-cs-fixer": "~1.10",
        "ecfectus/container": "dev-master",
        "ecfectus/http-message": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Ecfectus\\MiddlewareRunner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ecfectus\\MiddlewareRunner\\Test\\": "tests"
        }
    }
}
