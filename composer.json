{
    "name": "conformity/middleware-runner",
    "description": "A simple PSR container aware invokable middleware runner based on Relay Runner",
    "homepage": "https://github.com/conformity/middleware-runner",
    "license": "MIT",
    "authors": [
        {
            "name": "leemason",
            "email": "contact@nohalfpixels.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "container-interop/container-interop": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "fabpot/php-cs-fixer": "~1.10",
        "conformity/container": "dev-master",
        "conformity/http-message": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Conformity\\MiddlewareRunner\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Conformity\\MiddlewareRunner\\Test\\": "tests"
        }
    }
}
